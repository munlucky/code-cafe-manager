{
  "name": "codecafe",
  "version": "0.1.0",
  "private": true,
  "description": "AI CLI Orchestrator - Manage multiple AI CLI instances (baristas) in parallel",
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "clean": "pnpm -r clean",
    "typecheck": "pnpm -r typecheck",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "prepare": "husky"
  },
  "keywords": [
    "ai",
    "cli",
    "orchestrator",
    "claude",
    "codex",
    "workflow"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "eslint": "^8.55.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "hono": "^4.11.7",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "prettier": "^3.1.0",
    "type-coverage": "^2.29.7",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "pnpm": ">=8.0.0"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "electron",
      "electron-winstaller",
      "node-pty",
      "msw"
    ],
    "overrides": {
      "esbuild": ">=0.25.0",
      "jsonpath-plus": ">=10.3.0",
      "electron": ">=35.7.5",
      "webpack-dev-server": ">=5.2.1",
      "tmp": ">=0.2.4",
      "hono": ">=4.11.7",
      "tar": ">=7.5.7",
      "lodash": ">=4.17.23",
      "diff": ">=8.0.3"
    }
  },
  "lint-staged": {
    "packages/*/src/**/*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  }
}
