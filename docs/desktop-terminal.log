```typescript
// src/stores/scannerStore.ts
import { create } from 'zustand';
import { ScanResults, ScannerSettings } from '@/types';

interface ScannerState {
  // State
  isScanning: boolean;
  scanResults: ScanResults | null;
  lastScanTime: number | null;
  error: string | null;
  
  // Actions
  startScan: (options?: ScanOptions) => Promise<void>;
  stopScan: () => void;
  clearResults: () => void;
  setError: (error: string) => void;
}

export const useScannerStore = create<ScannerState>((set, get) => ({
  isScanning: false,
  scanResults: null,
  lastScanTime: null,
  error: null,
  
  startScan: async (options) => {
    set({ isScanning: true, error: null });
    try {
      const results = await Scanner.scan(options);
      set({ 
        scanResults: results, 
        lastScanTime: Date.now(),
        isScanning: false 
      });
    } catch (error) {
      set({ 
        error: error.message, 
        isScanning: false 
      });
    }
  },
  
  stopScan: () => {
    set({ isScanning: false });
  },
  
  clearResults: () => {
    set({ 
      scanResults: null, 
      lastScanTime: null,
      error: null 
    });
  },
  
  setError: (error) => {
    set({ error });
  },
}));
```

**Migration from EventBus:**
| Old EventBus Event | New Zustand Store Action |
|-------------------|-------------------------|
| `SCAN_STARTED` | `scannerStore.startScan()` |
| `SCAN_COMPLETED` | Auto-update store state |
| `SCAN_ERROR` | `scannerStore.setError()` |
| `SETTINGS_CHANGED` | `settingsStore.updateSettings()` |

