  353â†’    static handle(error, context = '') {\n   354â†’        const errorInfo = {\n   355â†’            message: error.message,\n   356â†’            stack: error.stack,\n   357â†’            context,\n   358â†’            timestamp: new Date().toISOString()\n   359â†’        };\n   360â†’        \n   361â†’        console.error('ğŸš¨ Error:', errorInfo);\n   362â†’        \n   363â†’        // ì—ëŸ¬ ë¡œê¹… ë° ë¦¬í¬íŒ…\n   364â†’        Logger.error('Application Error', errorInfo);\n   365â†’        \n   366â†’        // ì‚¬ìš©ì ì¹œí™”ì  ì—ëŸ¬ ë©”ì‹œì§€ í‘œì‹œ\n   367â†’        this.showUserFriendlyError(error);\n   368â†’    }\n   369â†’}\n   370â†’```\n   371â†’\n   372â†’#### 3. VolatilityCalculator (`calculator.js`)\n   373â†’ë˜ë¦¬ ìœŒë¦¬ì—„ìŠ¤ ë³€ë™ì„± ëŒíŒŒ ê³µì‹ì„ êµ¬í˜„í•©ë‹ˆë‹¤.\n   374â†’\n   375â†’```javascript\n   376â†’class VolatilityCalculator {\n   377â†’    static calculate(stockData, settings) {\n   378â†’        const { currentPrice, yesterdayClose, yesterdayHigh, yesterdayLow, volume } = stockData;\n   379â†’        \n   380â†’        // ì¼ì¼ ë³€ë™ì„± ê³„ì‚°\n   381â†’        const dailyRange = yesterdayHigh - yesterdayLow;\n   382â†’        const volatility = (dailyRange / yesterdayClose) * 100;\n   383â†’        \n   384â†’        // ë˜ë¦¬ ìœŒë¦¬ì—„ìŠ¤ ì§„ì…ê°€ ê³µì‹\n   385â†’        const entryPrice = yesterdayClose + (dailyRange * 0.6);\n   386â†’        \n   387â†’        // ë¦¬ìŠ¤í¬ ê´€ë¦¬\n   388â†’        const stopLoss = entryPrice * 0.95;    // 5% ì†ì ˆ\n   389â†’        const target1 = entryPrice * 1.02;     // 2% ëª©í‘œ\n   390â†’        const target2 = entryPrice * 1.05;     // 5% ëª©í‘œ\n   391â†’        \n   392â†’        // ì¡°ê±´ í™•ì¸\n   393â†’        const conditions = {\n   394â†’            volatilityOk: volatility >= 2 && volatility <= settings.volatilityMax * 100,\n   395â†’            volumeOk: volume >= settings.minVolume,\n   396â†’            priceOk: currentPrice >= settings.minPrice,\n   397â†’            hasBreakout: currentPrice >= entryPrice\n   398â†’        };\n   399â†’        \n   400â†’        return {\n   401â†’            entryPrice,\n   402â†’            stopLoss,\n   403â†’            target1,\n   404â†’            target2,\n   405â†’            volatility,\n   406â†’            hasBreakout: conditions.hasBreakout,\n   407â†’            isNearBreakout: !conditions.hasBreakout && (currentPrice / entryPrice) > 0.98,\n   408â†’            conditions\n   409â†’        };\n   410â†’    }\n   411â†’}\n   412â†’```\n   413â†’\n   414â†’#### 4. StorageManager (`storage.js`)\n   415â†’ë¡œì»¬ ìŠ¤í† ë¦¬ì§€ ê´€ë¦¬ë¥¼ ë‹´ë‹¹í•©ë‹ˆë‹¤.\n   416â†’\n   417â†’```javascript\n   418â†’class StorageManager {\n   419â†’    static KEYS = {\n   420â†’        RESULTS: 'sp500_results',\n   421â†’        SETTINGS: 'sp500_settings',\n   422â†’        CACHE: 'sp500_cache'\n   423â†’    };\n   424â†’\n   425â†’    static saveResults(results) {\n   426â†’        const data = {\n   427â†’            ...results,\n   428â†’            timestamp: new Date().toISOString()\n   429â†’        };\n   430â†’        localStorage.setItem(this.KEYS.RESULTS, JSON.stringify(data));\n   431â†’    }\n   432â†’\n   433â†’    static getResults() {\n   434â†’        const data = localStorage.getItem(this.KEYS.RESULTS);\n   435â†’        return data ? JSON.parse(data) : null;\n   436â†’    }\n   437â†’\n   438â†’    static getSettings() {\n   439â†’        const data = localStorage.getItem(this.KEYS.SETTINGS);\n   440â†’        return data ? JSON.parse(data) : {\n   441â†’            volatilityMax: 0.06,      // 6% ìµœëŒ€ ë³€ë™ì„±\n   442â†’            minVolume: 1000000,       // 100ë§Œì£¼ ìµœì†Œ ê±°ë˜ëŸ‰\n   443â†’            minPrice: 10,             // $10 ìµœì†Œ ê°€ê²©\n   444â†’            demoMode: true,           // ë°ëª¨ ëª¨ë“œ ê¸°ë³¸ê°’\n   445â†’            autoUpdateEnabled: false   // ìë™ ì—…ë°ì´íŠ¸ ê¸°ë³¸ê°’\n   446â†’        };\n   447â†’    }\n   448â†’\n   449â†’    static updateSettings(updat