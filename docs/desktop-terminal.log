// Lazy load modals
const SettingsModal = lazy(() => import('@/components/Settings/SettingsModal'));

// Memoize stock cards
const StockCard = memo(({ stock, variant, onClick }: StockCardProps) => {
  // Component implementation
}, (prev, next) => {
  return prev.stock.stock.ticker === next.stock.stock.ticker &&
         prev.stock.currentPrice === next.stock.currentPrice;
