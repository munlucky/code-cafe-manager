import type { AppSettings } from '../types/settings';

export class StorageManager {
  private static readonly SETTINGS_KEY = 'sp500_settings';
  private static readonly STOCKS_KEY = 'sp500_stocks';
  private static readonly LOGS_KEY = 'sp500_logs';

  static saveSettings(settings: AppSettings): void {
    try {
      localStorage.setItem(this.SETTINGS_KEY, JSON.stringify(settings));
    } catch (error) {
      console.error('Failed to save settings:', error);
      throw error;
    }
  }

  static getSettings(): AppSettings | null {
    try {
      const data = localStorage.getItem(this.SETTINGS_KEY);
      return data ? JSON.parse(data) : null;
    } catch (error) {
      console.error('Failed to load settings:', error);
      return null;
    }
  }

  // ... other methods with proper TypeScript types
}
