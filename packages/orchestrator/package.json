{
  "name": "@codecafe/orchestrator",
  "version": "0.1.0",
  "description": "Multi AI CLI Orchestrator Engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc && npm run copy:schemas",
    "copy:schemas": "node -e \"require('fs').cpSync('src/schema', 'dist/schema', {recursive: true, filter: (src) => src.endsWith('.json')})\"",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@codecafe/core": "workspace:*",
    "@codecafe/schema": "workspace:*",
    "@codecafe/providers-common": "workspace:*",
    "ajv": "^8.12.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "chokidar": "^3.5.3",
    "jsonpath-plus": "^7.2.0",
    "nanoid": "^5.0.4",
    "commander": "^11.1.0",
    "chalk": "^4.1.2",
    "ora": "^5.4.1",
    "js-yaml": "^4.1.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/js-yaml": "^4.0.9",
    "typescript": "^5.3.0"
  },
  "keywords": [
    "orchestrator",
    "workflow",
    "ai",
    "cli"
  ],
  "author": "CodeCafe Team",
  "license": "MIT"
}
