{
  "name": "@codecafe/orchestrator",
  "version": "0.1.0",
  "description": "Multi AI CLI Orchestrator Engine",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./session": "./dist/session/index.js",
    "./workflow-manager": "./dist/workflow-manager.js",
    "./workflow/workflow-executor": "./dist/workflow/workflow-executor.js",
    "./workflow/run-registry": "./dist/workflow/run-registry.js"
  },
  "scripts": {
    "clean": "powershell -Command \"Remove-Item -Recurse -Force dist, *.tsbuildinfo -ErrorAction SilentlyContinue\"",
    "build": "tsc && npm run copy:schemas",
    "copy:schemas": "node -e \"require('fs').cpSync('src/schema', 'dist/schema', {recursive: true, filter: (src) => src.endsWith('.json')})\"",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch",
    "test": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest --coverage",
    "test:cli": "npx tsx scripts/test-claude-cli.ts",
    "test:cli:spawn": "npx tsx scripts/test-claude-cli.ts spawn",
    "test:cli:prompt": "npx tsx scripts/test-claude-cli.ts prompt",
    "test:cli:full": "npx tsx scripts/test-claude-cli.ts full",
    "test:cli:i": "npx tsx scripts/test-claude-cli.ts interactive"
  },
  "dependencies": {
    "@codecafe/core": "workspace:*",
    "@codecafe/providers-common": "workspace:*",
    "@codecafe/schema": "workspace:*",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "gray-matter": "^4.0.3",
    "handlebars": "^4.7.8",
    "ink": "^4.4.1",
    "js-yaml": "^4.1.0",
    "jsonpath-plus": "^7.2.0",
    "nanoid": "^5.0.4",
    "node-pty": "^1.0.0",
    "ora": "^5.4.1",
    "react": "^18.2.0"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@vitest/coverage-v8": "^1.2.0",
    "ajv": "^8.17.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.2.0"
  },
  "keywords": [
    "orchestrator",
    "workflow",
    "ai",
    "cli"
  ],
  "author": "CodeCafe Team",
  "license": "MIT"
}
