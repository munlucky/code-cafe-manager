{
  "name": "@codecafe/desktop",
  "version": "0.1.0",
  "description": "CodeCafe Manager - Electron desktop application",
  "type": "module",
  "main": "./dist/packages/desktop/src/main/index.js",
  "scripts": {
    "start": "electron .",
    "build": "npm run build:main && npm run build:preload && npm run build:renderer",
    "build:main": "tsc -b",
    "build:preload": "tsc -b",
    "build:renderer": "webpack --config webpack.renderer.config.cjs --mode production",
    "dev": "npm run dev:start",
    "dev:start": "npm run build:main && npm run build:preload && concurrently -k -s first \"npm run dev:renderer\" \"npm run dev:main\" \"npm run dev:electron\"",
    "dev:main": "tsc -b --watch",
    "dev:renderer": "webpack serve --config webpack.renderer.config.cjs --mode development",
    "dev:electron": "wait-on -t 60000 http://localhost:8081 && electron .",
    "clean": "powershell -Command \"Remove-Item -Recurse -Force dist, *.tsbuildinfo -ErrorAction SilentlyContinue\"",
    "typecheck": "tsc --noEmit && tsc -p tsconfig.renderer.json --noEmit"
  },
  "keywords": [
    "codecafe",
    "electron",
    "desktop"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@codecafe/core": "workspace:*",
    "@codecafe/git-worktree": "workspace:*",
    "@codecafe/orchestrator": "workspace:*",
    "@codecafe/schema": "workspace:*",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-slot": "^1.2.4",
    "ansi-to-html": "^0.7.2",
    "clsx": "^2.1.0",
    "dotenv": "^16.4.5",
    "electron-squirrel-startup": "^1.0.0",
    "framer-motion": "^10.16.16",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.303.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.2.0",
    "yaml": "^2.3.4",
    "zod": "^3.22.4",
    "zustand": "^4.4.7"
  },
  "devDependencies": {
    "@electron-forge/cli": "^7.2.0",
    "@electron-forge/maker-deb": "^7.2.0",
    "@electron-forge/maker-rpm": "^7.2.0",
    "@electron-forge/maker-squirrel": "^7.2.0",
    "@electron-forge/maker-zip": "^7.2.0",
    "@electron-forge/plugin-auto-unpack-natives": "^7.2.0",
    "@electron-forge/plugin-webpack": "^7.2.0",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "autoprefixer": "^10.4.16",
    "buffer": "^6.0.3",
    "concurrently": "^8.2.2",
    "css-loader": "^6.8.1",
    "electron": "^28.0.0",
    "events": "^3.3.0",
    "html-webpack-plugin": "^5.6.0",
    "path-browserify": "^1.0.1",
    "postcss": "^8.4.32",
    "postcss-loader": "^8.2.0",
    "process": "^0.11.10",
    "shadcn": "^3.6.3",
    "stream-browserify": "^3.0.0",
    "style-loader": "^3.3.3",
    "tailwindcss": "^3.4.1",
    "ts-loader": "^9.5.1",
    "typescript": "^5.3.3",
    "util": "^0.12.5",
    "wait-on": "^9.0.3",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1"
  }
}
